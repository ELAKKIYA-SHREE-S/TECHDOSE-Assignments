#include <bits/stdc++.h>
using namespace std;
vector<int> bfs(int V, vector<int> adj[]) {
    int vis[V] = {0};
    vis[0] = 1;
    queue<int> q;
    q.push(0);
    vector<int> bfs;
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        bfs.push_back(node);
        for (auto it : adj[node]) {
            if (!vis[it]) {
                vis[it] = 1;
                q.push(it);
            }
        }
    }
    return bfs;
}
int main() {
    vector<int> adj[] = {{1, 4},{0, 2, 3},{1},{1},{0}};
    vector<int> ans = bfs(5, adj);
    for (int i = 0; i < ans.size(); i++) {cout << ans[i] << " ";}
    return 0;
}
